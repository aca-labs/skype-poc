{
  "name": "skype-poc",
  "version": "1.0.0",
  "description": "Quick tech test of native Skype client integration",
  "main": "src/main.js",
  "author": "Kim Burgess <kim@acaprojects.com>",
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "no-console": 0,
      "semi": 2
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "electron-prebuilt-compile": "1.6.7"
  },
  "dependencies": {
    "electron-compile": "^6.4.1",
    "electron-edge": "^6.5.5",
    "electron-prebuilt-compile": "1.6.6",
    "eslint": "^3.19.0",
    "skype-native": "^0.1.6"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
